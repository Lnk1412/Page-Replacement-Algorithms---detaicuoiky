#include <iostream>
#include <cstdlib>   // srand, rand
#include <ctime>     // time

using namespace std;

int main() {
    // Khởi tạo seed cho random theo thời gian thực
    srand(time(0));

    cout << "10 so ngau nhien tu 1 den 100: " << endl;

    for (int i = 0; i < 10; i++) {
        int number = rand() % 100 + 1;  // giá trị từ 1 -> 100
        cout << number << " ";
    }

    cout << endl;
    return 0;
}
